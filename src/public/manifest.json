{
    "manifest_version": 3,
    "name": "Meet Auto Recording",
    "description": "Google Meetの録画忘れ防止用拡張機能",
    "version": "1.0",
    "action": {
        "default_icon": "rec_icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/hqd-htwp-maw*"
            ],
            "js": [ "content_scripts.js" ]
        }
    ]
}